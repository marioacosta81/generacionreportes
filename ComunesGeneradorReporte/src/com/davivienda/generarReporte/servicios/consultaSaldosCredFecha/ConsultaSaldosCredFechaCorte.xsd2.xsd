    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ConsultaSaldosCredFechaCorteDataHeaderType">
    <xsd:sequence maxOccurs="1" minOccurs="1">
      <xsd:element default="ConsultaSaldosCredFechaCorte" minOccurs="1" name="nombreOperacion" type="xsd:string"/>
