  </xsd:complexType>
  <xsd:complexType name="ConsultaSaldosCredFechaCorteDataType">
    <xsd:all maxOccurs="1" minOccurs="1">
      <xsd:element name="numeroProducto" type="xsd:long"/>
    </xsd:all>
  </xsd:complexType>
  <xsd:element name="ConsultaSaldosCredFechaCorteResponse" type="tns:ConsultaSaldosCredFechaCorteResponseType"/>
  <xsd:complexType name="ConsultaSaldosCredFechaCorteResponseType">
