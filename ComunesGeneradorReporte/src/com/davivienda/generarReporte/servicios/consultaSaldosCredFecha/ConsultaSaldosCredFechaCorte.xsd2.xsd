<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.davivienda.com/xml/ConsultaSaldosCredFechaCorte" xmlns:tns="http://www.davivienda.com/xml/ConsultaSaldosCredFechaCorte" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:complexType name="ConsultaSaldosCredFechaCorteType">
